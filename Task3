int red1=8;
int green1=9;
int red2=10;
int green2=11;
int led=12;
int ledd=13;
int switchpin1=1;
int switchpin2=2;
void setup()
{
pinMode(red1,OUTPUT);
pinMode(green1,OUTPUT);
pinMode(red2,OUTPUT);
pinMode(green2,OUTPUT);
pinMode(led,OUTPUT);
pinMode(ledd,OUTPUT);
pinMode(switchpin1,INPUT);
pinMode(switchpin2,INPUT);
}

void loop()
{
  if(digitalRead(switchpin1)){
    digitalWrite(red1,1);
    digitalWrite(green1,1);
  }
  if(digitalRead(switchpin2)){
  digitalWrite(red2,1);
  digitalWrite(green2,1);
  }
  else{
    for( int i=8; i<=13; i++){
      analogWrite(i,0);
}}}
   
